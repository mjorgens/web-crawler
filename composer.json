{
  "name": "mjorgens/web-crawler",
  "description": "A PHP web crawler library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Marc Jorgensen",
      "email": "marcjorgensen@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.4",
    "guzzlehttp/psr7": "^1.6",
    "illuminate/database": "^6.8",
    "symfony/dom-crawler": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mjorgens\\Crawler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  }
}
